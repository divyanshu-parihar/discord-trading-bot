generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userRobinhoodTokens {
  id        Int    @unique @default(autoincrement())
  authorId  String @unique
  token     String @unique
  accountId String @unique
}
